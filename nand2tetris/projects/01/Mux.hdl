/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=outNot1); 
    And(a=a, b=outNot1, out=outAnd1);
    And(a=b, b=sel, out=outAnd2);
    Or(a=outAnd1, b=outAnd2, out=out);
}
